{
  "name": "aws-lambda-typescript",
  "version": "0.0.1",
  "description": "Some Project Description",
  "authors": [
    {
      "name": "Mark Kenneth Santos",
      "email": ""
    }
  ],
  "license": "private",
  "scripts": {
    "start:offline": "sls offline",
    "build-local": "sls package --package artifacts",
    "build-sbx": "set NODE_ENV=sbx&& sls package --package artifacts",
    "build-dev": "set NODE_ENV=dev&& sls package --package artifacts",
    "build-qa": "set NODE_ENV=qa&& sls package --package artifacts",
    "build-prod": "set NODE_ENV=prod&& sls package --package artifacts",
    "build-lambdas": "cd ci/tasks && sh build-local.sh",
    "deploy": "sls deploy",
    "typeorm": "typeorm-ts-node-commonjs",
    "create": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:generate -n Refactor",
    "lint": "eslint \"**/*.{ts,tsx}\" --quiet --fix",
    "lint:updated": "pipe-git-updated --ext=js -- eslint --ignore-path .gitignore",
    "prettier-check": "prettier -c --ignore-path .gitignore \"**/*.{css,html,js,json,md,yaml,yml}\"",
    "prettier-check:updated": "pipe-git-updated --ext=css --ext=html --ext=js --ext=json --ext=md --ext=yaml --ext=yml -- prettier -c",
    "prettify": "prettier --write --ignore-path .gitignore \"**/*.{css,html,js,json,md,yaml,yml}\"",
    "prettify:updated": "pipe-git-updated --ext=css --ext=html --ext=js --ext=json --ext=md --ext=yaml --ext=yml -- prettier --write",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --coverage",
    "start": "ts-node src/index.ts"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.245.0",
    "@aws-sdk/client-s3": "^3.245.0",
    "@aws-sdk/lib-storage": "^3.245.0",
    "@aws-sdk/s3-request-presigner": "^3.245.0",
    "@middy/core": "^4.0.10",
    "@middy/error-logger": "^4.0.10",
    "@middy/http-cors": "^4.0.10",
    "@middy/http-error-handler": "^4.0.10",
    "@middy/http-event-normalizer": "^4.0.10",
    "@middy/http-header-normalizer": "^4.0.10",
    "@middy/http-json-body-parser": "^4.0.10",
    "@middy/http-partial-response": "^4.0.10",
    "@middy/http-response-serializer": "^4.0.10",
    "@middy/http-security-headers": "^4.0.10",
    "busboy": "^1.6.0",
    "form-data": "^4.0.0",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.0.0",
    "pg": "^8.8.0",
    "tslib": "^2.4.1",
    "typeorm": "^0.3.11",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.226.0",
    "@faker-js/faker": "^7.6.0",
    "@jest/transform": "^29.0.2",
    "@types/aws-lambda": "^8.10.109",
    "@types/axios": "^0.14.0",
    "@types/busboy": "^1.5.0",
    "@types/jest": "^29.2.5",
    "@types/node": "^17.0.45",
    "@types/pg": "^8.6.6",
    "@types/serverless": "^3.12.9",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "aws-sdk-mock": "^5.7.0",
    "esbuild": "^0.16.16",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "json-schema-to-ts": "^2.6.2",
    "prettier": "^2.8.2",
    "serverless": "^3.26.0",
    "serverless-esbuild": "^1.37.0",
    "serverless-offline": "^12.0.4",
    "source-map-support": "^0.5.21",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.6.4"
  }
}
